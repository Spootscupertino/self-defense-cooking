{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UI Configuration",
  "type": "object",
  "properties": {
    "modules": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "settings": { "type": "object" }
        },
        "required": ["name"]
      }
    },
    "defaults": {
      "type": "object"
    }
  }
}
